{
    "name": "cheesegrits/filament-google-maps",
    "description": "A Google Maps package for Filament PHP with field, column and widget",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/cheesegrits/filament-google-maps",
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9",
        "guzzlehttp/guzzle": "^7.5",
        "geocoder-php/google-maps-provider": "^4.7",
        "php-http/guzzle7-adapter": "^1.0",
        "spatie/guzzle-rate-limiter-middleware": "^2.0",
        "php-http/message": "^1.13",
        "ryangjchandler/blade-capture-directive": "^0.2.2",
        "mastani/laravel-google-static-map": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Cheesegrits\\FilamentGoogleMaps\\": "src/",
            "Cheesegrits\\FilamentGoogleMaps\\Tests\\": "tests/src",
            "Cheesegrits\\FilamentGoogleMaps\\Tests\\Database\\Factories\\": "tests/database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cheesegrits\\FilamentGoogleMaps\\FilamentGoogleMapsServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Hugh Messenger",
            "email": "hugh.messenger@gmail.com"
        }
    ],
    "require-dev": {
        "filament/filament": "^2.0",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.3",
        "orchestra/testbench": "^7.11",
        "pestphp/pest-plugin-livewire": "^1.0",
        "nonsapiens/realaddressfactory": "^2.0",
        "timacdonald/log-fake": "^2.0",
        "laravel/pint": "^1.5"
    },
    "scripts": {
	    "style": "vendor/bin/pint"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    }
}
